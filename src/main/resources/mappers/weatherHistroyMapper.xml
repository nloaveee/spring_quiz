<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.weather_history.mapper.WeatherHistoryMapper">
 	<select id="selectWeatherHistoryList" resultType="com.quiz.weather_history.domain.WeatherHistory">
 		SELECT 
 			`id`
 			,`date`
			,`weather`
			,`temperatures`
			,`precipitation`
			,`microDust`
			,`windSpeed`
			,`createdAt`
			,`updatedAt`
		FROM
			`weather_history` 		
 	</select>
 	
 	<insert id="insertWeather" parameterType="com.quiz.weather_history.domain.WeatherHistory"
 	useGeneratedKeys="true" keyProperty="id">
 		INSERT INTO `weather_history`
 		(	
 			`date`
			,`weather`
			,`temperatures`
			,`precipitation`
			,`microDust`
			,`windSpeed`
			,`createdAt`
			,`updatedAt`
		)
 		VALUE
 		(
 			#{date}
 			, #{weather}
 			, #{temperatures}
 			, #{precipitation}
 			, #{microDust}
 			, #{windSpeed}
 			, NOW()
 			, NOW()
 			
 		)
 	
 	
 	</insert>
 
 </mapper>